{
  "name": "@wincoins/backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "indexer": "tsx watch src/indexer/index.ts",
    "indexer:init": "tsx src/indexer/backfill.ts",
    "indexer:continuous": "tsx watch src/indexer/continuous.ts",
    "indexer:continuous:init": "tsx src/indexer/continuous-init.ts",
    "oracle:continuous": "tsx watch src/oracle/continuous-oracle.ts",
    "oracle:dota": "tsx watch src/oracle/dota-oracle-runner.ts",
    "oracle:ml-dota": "tsx watch src/oracle/ml-dota-oracle-runner.ts",
    "scheduler:tick": "tsx src/scheduler/tick.ts",
    "scheduler": "while true; do npm run scheduler:tick; sleep 60; done",
    "create-round": "tsx src/scripts/create-test-round.ts",
    "create-round:1min": "DURATION=60 tsx src/scripts/create-test-round.ts",
    "create-round:5min": "DURATION=300 tsx src/scripts/create-test-round.ts",
    "console": "tsx src/console.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "NODE_ENV=test vitest run",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push --config=drizzle.config.ts",
    "db:studio": "drizzle-kit studio",
    "db:clean": "tsx src/db/clean.ts"
  },
  "dependencies": {
    "@adraffy/ens-normalize": "^1.10.1",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^10.0.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.69.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.211.0",
    "@opentelemetry/resources": "^2.5.0",
    "@opentelemetry/sdk-node": "^0.211.0",
    "@opentelemetry/semantic-conventions": "^1.39.0",
    "@trpc/server": "^11.0.0-rc.553",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.44.0",
    "ethers": "^6.15.0",
    "fastify": "^5.2.0",
    "jsonwebtoken": "^9.0.3",
    "postgres": "^3.4.5",
    "ws": "^8.19.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.9.1",
    "@types/ws": "^8.18.1",
    "better-sqlite3": "^11.8.1",
    "drizzle-kit": "^0.31.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
